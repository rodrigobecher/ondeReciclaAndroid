<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="senac.tcc.rodrigo.onderecicla">

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="senac.tcc.rodrigo.ondeReciclaAndroid.MainActivity" />
        <activity android:name="senac.tcc.rodrigo.ondeReciclaAndroid.senac.tcc.rodrigo.onderecicla.listaEmpresa" />
        <activity android:name="senac.tcc.rodrigo.ondeReciclaAndroid.activity_empresa"></activity>
        <activity
            android:name="senac.tcc.rodrigo.ondeReciclaAndroid.LoginActivity"
            android:label="@string/title_activity_login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="senac.tcc.rodrigo.ondeReciclaAndroid.cadastrarCliente"></activity>
        <activity android:name="senac.tcc.rodrigo.ondeReciclaAndroid.senac.tcc.rodrigo.onderecicla.MenuCliente"></activity>
    </application>

</manifest>